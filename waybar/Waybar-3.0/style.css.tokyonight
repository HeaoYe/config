
* {
  padding: 0;
  margin: 0;
  font-family: JetBrainsMono Nerd Font;
  font-weight: bold;
  font-size: 16px;
  background-color: rgba(30,32,48,1); /* Reduced opacity background */ 
}

.modules-right {
  /* background-color: rgba(30,32,48,1); */
  border-radius: 5px 0px 0px 5px;
}

window#waybar {
  background-color: rgba(0, 0, 0, 0);
}

#custom-launcher {
  font-size: 20px;
  color: #00bfff;
  padding-right: 10px;
  background-color: rgba(30,32,48,1);
}

