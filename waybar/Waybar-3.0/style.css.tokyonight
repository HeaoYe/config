#cpu,
#memory,
#temperature {
  font-size: 15px;
  background: #363a4f;
  margin-top: 5px;
  margin-bottom: 5px;
}

#cpu {
  color: #eed49f;
  padding: 0px 10px 0px 12px;
  border-radius: 5px 0px 0px 5px;
}

#memory {
  color: #a6da95;
  padding: 0px 10px 0px 0px;
}

#temperature {
  color: #8aadf4;
  padding: 0px 12px 0px 0px;
  border-radius: 0px 5px 5px 0px;
}

#custom-window-name {
  color: #68b0d6;
  margin-right: 10px;
  background-color: rgba(30,32,48,1);
}

#custom-pacman-update-icon {
  background-color: #68b0d6;
  color: #181926;
  padding-left: 5px;
  padding-right: 5px;
  margin-top: 5px;
  margin-bottom: 5px;
  border-radius: 5px;
}

#custom-pacman-update {
  color: #8bd5ca;
  margin: 5px 10px 5px 0px;
}

#workspaces button:nth-child(1) label {
  color: #8aadf4;
  margin: 0px 8px;
  background-color: rgba(30,32,48,1);
}

#workspaces button:nth-child(2) label {
  color: #ed8796;
  margin: 0px 8px;
  background-color: rgba(30,32,48,1);
}

#workspaces button:nth-child(3) label {
  color: #a6da95;
  margin: 0px 8px;
  background-color: rgba(30,32,48,1);
}

#workspaces button:nth-child(4) label {
  color: #c6a0f6;
  margin: 0px 8px;
  background-color: rgba(30,32,48,1);
}

#workspaces button:nth-child(5) label {
  color: #f4dbd6;
  margin: 0px 8px;
  background-color: rgba(30,32,48,1);
}

#workspaces button:nth-child(6) label {
  color: #f5a97f;
  margin: 0px 8px;
  background-color: rgba(30,32,48,1);
}

#workspaces button:nth-child(7) label {
  color: #8bd5ca;
  margin: 0px 8px;
  background-color: rgba(30,32,48,1);
}

#workspaces button:nth-child(8) label {
  color: #68b0d6;
  margin: 0px 8px;
  background-color: rgba(30,32,48,1);
}

#workspaces button:nth-child(9) label {
  color: #aab1d3;
  margin: 0px 8px;
  background-color: rgba(30,32,48,1);
}


#battery {
  color: #a6da95;
  /* margin: 0px 10px 0px 0px; */
}

#pulseaudio,
#backlight {
  font-size: 15px;
  background-color: #363a4f;
  margin-top: 5px;
  margin-bottom: 5px;
}

#backlight {
  color: #eed49f;
  padding: 0px 10px 0px 10px;
  border-radius: 5px 0px 0px 5px;
}

#pulseaudio {
  color: #91d7e3;
  padding: 0px 10px 0px 0px;
  border-radius: 0px 5px 5px 0px;
}

#custom-right-arr {
  color: #8aadf4;
  background-color: rgba(30,32,48,1);
}

#custom-pacman {
  color: #68b0d6;
  background-color: rgba(30,32,48,1);
}

#network {
  color: #c6a0f6;
  margin-right: 10px;
  background-color: rgba(30,32,48,1);
}

#custom-dot {
  color: #6e738d;
  padding-right: 10px;
  background-color: rgba(30,32,48,1);
}

#custom-dot-alt {
  color: #a5adcb;
  padding-left: 10px;
  padding-right: 10px;
}

#disk {
  color: #68b0d6;
  background-color: rgba(30,32,48,1);
}

#custom-clock-icon {
  color: #cad3f5;
  padding-left: 10px;
  padding-right: 10px;
  margin-top: 5px;
  margin-bottom: 5px;
  border-radius: 5px;
}

#custom-filler {
  color: #6e738d;
  padding-left: 0px;
  padding-right: 9px;
  margin-top: 5px;
  margin-bottom: 5px;
  border-radius: 5px;
}

#custom-filler2 {
  color: #6e738d;
  padding-left: 3px;
  padding-right: 9px;
  margin-top: 5px;
  margin-bottom: 5px;
  border-radius: 5px;
}

#clock {
  padding-left: 10px;
}

